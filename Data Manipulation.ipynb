    "def cov_cor(sentiment, logReturns):\n",
    "    '''Returns a tuple containing the correlation and\n",
    "    covariance respectively between two timeseries dataframes'''\n",
    "    cor = sentiment.corr(logReturns)\n",
    "    cov = sentiment.cov(logReturns) \n",
    "    return (cor, cov)"
   ]
